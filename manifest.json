{
  "manifest_version": 2,

  "name": "Cypher Queries Exporter",
  "description": "This extension prompts a download of neo4j cypher queries",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title":"Export  Personal Cypher Queries"
  },

  "permissions": [
    "tabs",
    "activeTab",
    "downloads",
    "<all_urls>"
  ]
}